var amigos = $("#amigos .row");
var amigo = amigos.find("[data-container-amigo='" + "<%= @amigo.id %>" + "']");
amigo.remove();

var bloqueados = $("#bloqueados .row");
bloqueados.append("<%= j(render 'amigos/amigo', amigo: @amigo) %>");

var contador_amigos = parseInt($("#contador_amigos").text());
$("#contador_amigos").text(contador_amigos - 1);

var contador_bloqueados = parseInt($("#contador_bloqueados").text());
$("#contador_bloqueados").text(contador_bloqueados + 1);

var personas = $("#personas .row");
var persona = personas.find("[data-container-amigo='" + "<%= @amigo.id %>" + "']");
persona.remove();
personas.append("<%= j(render 'amigos/amigo', amigo: @amigo) %>");